{
    "version": 1,
    "schedule": {
        "type": "simple",
        "interval": "1s"
    },
    "workflow": {
        "collect": {
            "metrics": {
                "/intel/exec/cpu0_load": {},
                "/intel/exec/disk_io_wait": {},
                "/intel/exec/net_eth0_util_percent": {},
                "/intel/exec/vm_used_percent": {}
           },
            "config": {
            },
            "process": null,
            "publish": [
                {
                    "plugin_name": "influx",                            
                    "config": {
                        "host": "127.0.0.1",
                        "port": 8086,
                        "database": "snap",
                        "user": "admin",
                        "password": "admin"
                    }
                }
            ]
        }
    }
}
